    "def heightChecker(heights):\n",
    "    ans = 0\n",
    "    x = sorted(heights)\n",
    "    y = heights\n",
    "    for i in range(len(x)):\n",
    "        if x[i]!=y[i]:\n",
    "            ans+=1\n",
    "    return ans"
   ]
